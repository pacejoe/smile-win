{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Smile @ Camp Win"
  },
  "description": "This is a chrome extension to ensure you use smile (hopefully for camp win) while making purchases from Amazon",
  "icons": {
    "16": "images/icon_16.png",
    "19": "images/icon_19.png",
    "24": "images/icon_24.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "64": "images/icon_64.png",
    "72": "images/icon_72.png",
    "96": "images/icon_96.png",
    "128": "images/icon_128.png"
  },
  "manifest_version": 2,
  "name": "Smile @ Camp Win",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://www.amazon.com/*",
    "https://www.amazon.com/*"
  ],
  "version": "1.0"
}